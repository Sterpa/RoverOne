form.queue__form
    fieldset.queue__fieldset
        legend= translation.queue_legend[lang]
        each val, key in data.gui
            textarea.queue__gui-fieldname(
                readonly
                disabled
                rows='1'
            )= translation[`${key}_fieldname`][lang]
            span &#160;
        <br>
        each val, key in data.gui
            textarea.queue__gui(
                readonly
                name=key
                placeholder=translation[`${key}_placeholder`][lang]
            )= JSON.stringify(data.gui[key])
            span &#160;
        <br>
        <br>
        each val, key in data.dev
            textarea.queue__dev-fieldname(
                readonly
                disabled
                rows='1'
            )= translation[`${key}_fieldname`][lang]
            span &#160;
        <br>
        each val, key in data.dev
            textarea.queue__dev(
                readonly
                name=key
                placeholder=translation[`${key}_placeholder`][lang]
            )= JSON.stringify(data.dev[key])
            span &#160;
        <br>
        button.queue__button(
            type='submit'
        )= translation.queue_button_submit[lang]
        span &#160;
        button.queue__buttonStart(
            type='button'
            data-action='run'
            style='color: ' + runColor
        )= translation.queue_button_start[lang]
        span &#160;